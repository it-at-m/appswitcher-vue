{
  "name": "@muenchen/appswitcher-vue",
  "version": "0.0.1",
  "description": "Vue.js/Vuetify component for appswitcher-server",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/appswitcher.umd.cjs",
  "module": "./dist/appswitcher.js",
  "exports": {
    ".": {
      "import": "./dist/appswitcher.js",
      "require": "./dist/appswitcher.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:demo": "vue-tsc && vite build -c vite.demo.config.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@mdi/font": "> 5",
    "vue": "^2.7.14",
    "vuetify": "^2.6.12"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/test-utils": "^1.3.3",
    "jsdom": "^21.1.1",
    "path": "^0.12.7",
    "sass": "~1.62",
    "semantic-release": "^19.0.5",
    "typescript": "^5.0.4",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.0.0",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.0.11"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}